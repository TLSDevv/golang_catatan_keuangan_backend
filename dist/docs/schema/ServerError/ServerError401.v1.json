{
    "title": "Server Error Message",
    "type": "object",
    "description": "Error returned by server",
    "properties":{
        "code":{
            "type": "integer",
            "description": "Error code",
            "example": "401"
        },
        "message":{
            "type": "string",
            "description": "Error message",
            "example": "Unauthorized"
        }
    },
    "required": ["message", "code"]
}